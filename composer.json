{
    "name": "xjchen/oauth2-wechat",
    "description": "Wechat OAuth 2.0 Client Provider for The PHP League OAuth2-Client",
    "license": "MIT",
    "authors": [
        {
            "name": "Xujian Chen",
            "email": "xchenxujian@126.com",
            "homepage": "http://www.echo58.com"
        }
    ],
    "keywords": [
        "oauth",
        "oauth2",
        "client",
        "authorization",
        "authentication",
        "wechat"
    ],
    "require": {
        "php": ">=5.4.0",
        "ext-curl": "*",
        "egeloen/http-adapter": "0.6.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "mockery/mockery": "~0.9",
        "squizlabs/php_codesniffer": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "Xjchen\\OAuth2\\Client\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Xjchen\\OAuth2\\Client\\Test\\": "tests/src/"
        }
    },
    "branch-alias": {
        "dev-master": "0.0.x-dev"
    }
}
